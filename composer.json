{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yii2tech/ar-softdelete": "^1.0",
        "aracoool/yii2-uuid": "^1.1",
        "kartik-v/yii2-widget-datepicker": "dev-master",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-field-range": "*",
        "kartik-v/yii2-widget-activeform": "dev-master",
        "yiisoft/yii2-jui": "^2.0",
        "nategood/httpful": "^0.2.20",
        "kartik-v/yii2-widget-select2": "@dev",
        "la-haute-societe/yii2-save-relations-behavior": "*",
        "dektrium/yii2-user": "*",
        "dektrium/yii2-rbac": "*",
        "yiisoft/yii2-queue": "^2.3",
        "kartik-v/yii2-widget-rating": "*",
        "zendesk/zendesk_api_client_php": "dev-master",
        "mihaildev/yii2-elfinder": "*",
        "mihaildev/yii2-ckeditor": "dev-master",
        "kartik-v/yii2-date-range": "dev-master",
        "yiisoft/yii2-imagine": "dev-master",
        "yiisoft/yii2-redis": "dev-master",
        "yii2tech/csv-grid": "dev-master",
        "aneeshikmat/yii2-time-down-counter": "v1.0.0-stable",
        "kartik-v/yii2-widget-typeahead": "dev-master",
        "2amigos/yii2-file-upload-widget": "~1.0",
        "kartik-v/yii2-mpdf": "dev-master",
        "kartik-v/yii2-widget-depdrop": "dev-master",
        "yii2mod/yii2-cart": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "insolita/yii2-migration-generator": "~3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
