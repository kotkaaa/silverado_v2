{
    "name": "kotkaaa/silverado_v2",
    "description": "Silverado website",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-imagine": "dev-master",
        "yiisoft/yii2-redis": "dev-master",
        "yiisoft/yii2-queue": "dev-master",
        "kartik-v/yii2-widget-datepicker": "dev-master",
        "kartik-v/yii2-widget-datetimepicker": "dev-master",
        "kartik-v/yii2-field-range": "dev-master",
        "kartik-v/yii2-widget-activeform": "dev-master",
        "kartik-v/yii2-widget-select2": "dev-master",
        "kartik-v/yii2-widget-rating": "dev-master",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-mpdf": "dev-master",
        "kartik-v/yii2-widget-depdrop": "dev-master",
        "kartik-v/yii2-widget-typeahead": "dev-master",
        "dektrium/yii2-user": "*",
        "dektrium/yii2-rbac": "*",
        "mihaildev/yii2-elfinder": "*",
        "mihaildev/yii2-ckeditor": "dev-master",
        "yii2tech/ar-softdelete": "^1.0",
        "yii2mod/yii2-cart": "*",
        "aracoool/yii2-uuid": "^1.1",
        "nategood/httpful": "^0.2.20",
        "la-haute-societe/yii2-save-relations-behavior": "*",
        "yii2tech/csv-grid": "dev-master",
        "aneeshikmat/yii2-time-down-counter": "v1.0.0-stable",
        "2amigos/yii2-file-upload-widget": "~1.0",
        "lis-dev/nova-poshta-api-2": "dev-master",
        "integready/yii2-turbosms": "*",
        "phpoffice/phpspreadsheet": "*",
        "coderius/yii2-swiper-slider": "^1.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/codeception": "*",
        "codeception/module-asserts": "*",
        "codeception/module-yii2": "*",
        "codeception/module-filesystem": "*",
        "codeception/verify": "*",
        "symfony/browser-kit": "*",
        "insolita/yii2-migration-generator": "*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
